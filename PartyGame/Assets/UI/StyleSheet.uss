    flex-direction: column;
    flex-grow: 0;
}

.iconImage {
    flex-shrink: 0;
    align-items: auto;
    justify-content: center;
    align-self: center;
    align-content: auto;
    width: 64px;
    height: 64px;
    margin-top: 0;
    background-image: url("project://database/Assets/Images/UI/joystick.png?fileID=21300000&guid=33732345d7365b24b93f896c89b523b4&type=3#joystick");
